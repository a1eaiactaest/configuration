syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set background=dark
set ruler
colorscheme peachpuff
highlight Comment ctermfg=green
highlight Search ctermbg=yellow ctermfg=black guibg=yellow guifg=black
highlight Visual cterm=reverse ctermbg=NONE ctermfg=NONE gui=reverse guibg=NONE guifg=NONE
